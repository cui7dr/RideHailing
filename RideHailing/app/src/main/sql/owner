--创建车主表
DROP TABLE IF EXISTS "owner";
CREATE TABLE "owner" (
  "id" INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
  "name" TEXT,
  "sex" TEXT,
  "birth" TEXT,
  "country" TEXT,
  "plate" TEXT,
  "address" TEXT,
  "license" TEXT,
  "model" TEXT,
  "start" TEXT,
  "end" TEXT,
  "createTime" TEXT,
  "updateTime" TEXT,
  "spare1" TEXT,
  "spare2" TEXT,
  "spare3" TEXT
);

PRAGMA foreign_keys = true;